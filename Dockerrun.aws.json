{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [{
			"name": "client",
			"image": "kalpithakm/multi-client",
			"hostname": "client",
			"essential": false,
			"memory" : 128
		},
		{
			"name": "server",
			"image": "kalpithakm/multi-server",
			"hostname": "api",
			"essential": false,
			"memory" : 128
		},
		{
			"name": "worker",
			"image": "kalpithakm/multi-worker",
			"hostname": "worker",
			"essential": false,
			"memory" : 128
		},
		{
			"name": "nginx",
			"image": "kalpithakm/multi-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMapping": [{
				"hostPort": 80,
				"containerPort": 80
			}],
			"links": ["client", "server"],
			"memory" : 128
		}
	]
}